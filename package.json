{
    "name": "billar",
    "version": "0.0.12",
    "description": "Billing application for remote work",
    "main": "app/main.js",
    "scripts": {
      "preclear": "pnpm run prebuild",
      "clear": "rimraf ./node_modules ./test ./dist ./coverage ./coverage-e2e ./e2e/screenshots ./package-lock.json ./gulpfile.js ./gulpfile.js.map",
      "postinstall": "tsc -p ./",
      "prebuild": "rimraf ./app/*.js ./app/*.map",
      "build": "pnpm run script",
      "script": "tsc --project ./src",
      "predist": "pnpm run build",
      "dist": "gulp --cwd .",
      "start": "electron ./",
      "pretest": "rimraf ./coverage",
      "test": "jest --env=enzyme --setupFilesAfterEnv=jest-enzyme --reporters=jest-html-reporter jest-spec-reporter jest-junit --coverage --coverageDirectory ../test/coverage --rootDir ./src",
      "pree2e": "rimraf ./test/coverage-e2e ./e2e/screenshots",
      "e2e": "jest --runInBand --coverage --coverageDirectory ../test/coverage-e2e --rootDir ./e2e"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/maxgdn/billar.git"
    },
    "keywords": [
      "electron",
      "gulp",
      "jest",
      "react",
      "styled",
      "typescript"
    ],
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged && npm run test",
        "pre-push": "npm run e2e"
      }
    },
    "lint-staged": {
      "*.{ts,tsx}": [
        "eslint --fix --quiet",
        "git add"
      ]
    },
    "jest-html-reporter": {
      "pageTitle": "ts-react-playground",
      "outputPath": "./test/index.html"
    },
    "jest-junit": {
      "outputDirectory": "./test"
    },
    "jest": {
      "collectCoverageFrom": [
        "**/*.{ts,tsx}",
        "**/*.{js,jsx}",
        "!**/node_modules/**",
        "!main.ts",
        "!renderer.tsx"
      ],
      "coverageReporters": [
        "lcovonly",
        "html",
        "text",
        "text-summary"
      ],
      "coverageThreshold": {
        "global": {
          "branches": 100,
          "functions": 100,
          "lines": 100,
          "statements": 100
        }
      },
      "moduleFileExtensions": [
        "ts",
        "tsx",
        "js"
      ],
      "transform": {
        "\\.ts(x)?$": "ts-jest"
      }
    },
    "author": "MaxGDN",
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/maxgdn/billar/issues"
    },
    "homepage": "https://github.com/maxgdn/billar",
    "dependencies": {
      "electron": "9.2.1",
      "fancy-log": "1.3.3",
      "react": "16.13.1",
      "react-dom": "16.13.1"
    },
    "devDependencies": {
      "@types/archiver": "3.1.0",
      "@types/electron-packager": "15.0.1",
      "@types/enzyme": "3.10.5",
      "@types/enzyme-adapter-react-16": "1.0.6",
      "@types/fancy-log": "1.3.1",
      "@types/gulp": "4.0.6",
      "@types/gulp-sass": "4.0.0",
      "@types/gulp-sourcemaps": "0.0.32",
      "@types/jest": "26.0.10",
      "@types/react": "16.9.46",
      "@types/react-dom": "16.9.8",
      "@types/react-test-renderer": "16.9.3",
      "@types/rimraf": "3.0.0",
      "@types/through2": "2.0.36",
      "@typescript-eslint/eslint-plugin": "3.9.1",
      "@typescript-eslint/parser": "3.9.1",
      "archiver": "5.0.0",
      "del": "5.1.0",
      "electron-packager": "15.0.0",
      "enzyme": "3.11.0",
      "enzyme-adapter-react-16": "1.15.3",
      "eslint": "7.7.0",
      "eslint-config-prettier": "6.11.0",
      "eslint-plugin-jest": "23.20.0",
      "eslint-plugin-prettier": "3.1.4",
      "eslint-plugin-react": "7.20.6",
      "eslint-plugin-react-hooks": "4.1.0",
      "eslint-plugin-unicorn": "21.0.0",
      "generate-changelog": "1.8.0",
      "gulp": "4.0.2",
      "gulp-git": "2.10.1",
      "husky": "4.2.5",
      "jest": "26.4.0",
      "jest-cli": "26.4.0",
      "jest-environment-enzyme": "7.1.2",
      "jest-enzyme": "7.1.2",
      "jest-html-reporter": "3.1.3",
      "jest-image-snapshot": "4.1.0",
      "jest-junit": "11.1.0",
      "jest-puppeteer": "4.4.0",
      "jest-spec-reporter": "1.0.14",
      "lint-staged": "10.2.11",
      "node-sass": "4.14.1",
      "prettier": "2.0.5",
      "react-test-renderer": "16.13.1",
      "rimraf": "3.0.2",
      "spectre.css": "0.5.9",
      "spectron": "11.1.0",
      "through2": "4.0.2",
      "ts-jest": "26.2.0",
      "typescript": "3.9.7"
    }
  }